{
  "name": "spacedoc",
  "version": "0.3.0",
  "description": "Documentation generator that can combine data from multiple parsers, such as SassDoc and JSDoc.",
  "keywords": [
    "documentation",
    "generator",
    "sassdoc",
    "jsdoc"
  ],
  "main": "index.js",
  "bin": {
    "spacedoc": "./bin/spacedoc.js"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "rm -rf docs",
    "docs": "node ./bin/spacedoc.js --watch",
    "predocs": "npm run clean",
    "server": "lite-server",
    "start": "npm run docs & npm run server",
    "test": "mocha"
  },
  "author": "Geoff Kimball <geoff@geoffkimball.com> (http://geoffkimball.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/spacedoc/spacedoc.git"
  },
  "bugs": {
    "url": "https://github.com/spacedoc/spacedoc/issues"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "escape-html": "^1.0.3",
    "flexiconfig": "^2.0.0",
    "front-matter": "^2.3.0",
    "glob": "^7.1.2",
    "glob-parent": "^3.1.0",
    "glob-watcher": "^3.2.0",
    "gulp-util": "^3.0.8",
    "highlight.js": "^9.12.0",
    "is-empty-object": "^1.1.1",
    "js-yaml": "^3.11.0",
    "jsdoc-api": "^2.0.6",
    "lodash.filter": "^4.6.0",
    "lodash.find": "^4.6.0",
    "lodash.findindex": "^4.6.0",
    "lodash.frompairs": "^4.0.1",
    "markdown-it": "^8.4.1",
    "meow": "^4.0.0",
    "mkdirp": "^0.5.1",
    "pify": "^2.3.0",
    "portatheme": "^0.3.0",
    "pug": "^2.0.3",
    "random-id": "0.0.2",
    "replace-basename": "^1.1.0",
    "replace-ext": "^1.0.0",
    "rimraf": "^2.6.2",
    "sassdoc": "^2.5.0",
    "spacedoc-helpers": "^0.3.0",
    "strip-indent": "^2.0.0",
    "through2": "^2.0.3",
    "transform-markdown-links": "^1.1.0",
    "vinyl": "^2.1.0",
    "vinyl-fs": "^2.4.4"
  },
  "devDependencies": {
    "chai-as-promised": "^6.0.0",
    "cheerio": "^0.22.0",
    "jstransformer-markdown-it": "^2.0.0",
    "lite-server": "^2.3.0",
    "mock-require": "^2.0.2",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.14.0",
    "spacedoc-theme-default": "^0.3.0"
  },
  "xo": false
}
